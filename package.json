{
  "name": "react-leaf",
  "private": true,
  "description": "React leaf mono repo",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Nans Laupretre <nans.laupretre@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "test": "vitest --watch false",
    "test:cc": "vitest --coverage --watch false",
    "build": "turbo run build",
    "lint": "eslint \"**/*.{ts,tsx}\" --quiet",
    "fix": "eslint \"**/*.{ts,tsx}\" --fix --quiet",
    "check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "validate": "yarn format && yarn lint && yarn check"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "clsx": "^2.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react-icons": "^4.11.0",
    "remark-gfm": "^3.0.1",
    "sass": "^1.69.4",
    "storybook": "^7.5.1",
    "storybook-css-modules": "^1.0.8",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@3.6.3"
}
